{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "target": "ES6",
      "module": "Node16",
      "moduleResolution": "Node16",
      "removeComments": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "strict": true
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules", "build", "./**/*.js"]
  }